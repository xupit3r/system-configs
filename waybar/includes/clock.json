{
  "clock": {
    "format": "<b>{:%OI:%M %p}</b>",
    "format-alt": "<b>{:%a, %b %d %OI:%M %p}</b>",
    "tooltip-format": "<tt><big>{calendar}</big></tt>",
    "calendar": {
      "mode": "month",
      "on-scroll": 1,
      "on-click-right": "mode",
      "format": {
        "months": "<span color='#7dcfff'><b>{}</b></span>",
        "days": "<span color='#e0af68'><b>{}</b></span>",
        "weeks": "<span color='#7dcfff'><b>W{}</b></span>",
        "weekdays": "<span color='#bb9af7'><b>{}</b></span>",
        "today": "<span color='#f7768e'><b><u>{}</u></b></span>"
      }
    }
  }
}
